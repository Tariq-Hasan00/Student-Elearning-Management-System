#include <stdio.h>
int main()
{
    int x = 5;
    int flag = 0;
    int ar[10];
    for(int i = 0; i < 10; i++){
        scanf("%d", &ar[i]);
    }
    //for(int i = 0; i < 10; i++){
   //         ar[i] = ar[i] + x;
    //}
    for(int i = 0; i < 10; i++){
            if(ar[i] % 2 == 0)
            {
                flag = 1;
                break;
            }

    }
    if(flag == 1){
            printf("yes");
    } else {
         printf("No");
    }
    return 0;
}
